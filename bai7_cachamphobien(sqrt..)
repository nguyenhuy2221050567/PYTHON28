from math import * 
#sqrt : square root // ham can bac hai lay ca so thap phan 
print(sqrt(36))
#isqrt : integer square root 
print(isqrt(39))
print(isqrt(16))
#pow : power : luy thua 
print(pow(2,10))
#ceil : lam tron len 
print(ceil(7.8))
#floor: lam tron xuong 
print(floor(7.8))
#factorial: giai thua 
printt(factorrial(10))
#gcd(a,b) uoc chung lon nhat 
print(gcd(a, b ))
#comb (n , k ) to hop chap k cua n 
print(comb(2 , 10 ))
#perm : hoan vi 
print(perm(5,3))
#fabs : tri tuyet doi 
#round : lam tron phu thuoc vao so thap phan 
#max
print(max(2,3,4,5))
#sum
